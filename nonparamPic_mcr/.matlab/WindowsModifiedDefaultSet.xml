<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Windows" modifieddefault="true">
   <Context id="Global">
      <Action id="caret-end">
         <Stroke alt="on" code="VK_DOWN"/>
         <Stroke code="VK_DOWN" meta="on"/>
         <Stroke alt="on" code="VK_KP_DOWN"/>
         <Stroke code="VK_KP_DOWN" meta="on"/>
      </Action>
      <Action id="comparisons-next"/>
      <Action id="prev-cell"/>
      <Action id="page-down">
         <Stroke code="VK_DOWN" ctrl="on"/>
         <Stroke code="VK_KP_DOWN" ctrl="on"/>
      </Action>
      <Action id="next-message"/>
      <Action id="caret-begin">
         <Stroke alt="on" code="VK_UP"/>
         <Stroke code="VK_UP" meta="on"/>
         <Stroke alt="on" code="VK_KP_UP"/>
         <Stroke code="VK_KP_UP" meta="on"/>
      </Action>
      <Action id="previous-message"/>
      <Action id="comparisons-previous"/>
      <Action id="next-cell"/>
      <Action id="page-up">
         <Stroke code="VK_UP" ctrl="on"/>
         <Stroke code="VK_KP_UP" ctrl="on"/>
      </Action>
   </Context>
</CustomKeySet>